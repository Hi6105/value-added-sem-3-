#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void solve(string s,int cnt,int a){
    int b=0,temp=cnt; 
    bool flag=false;
    for(int i=0;i<a&&temp;i++){
        if(s[i]!=' ')
            b++;
        else
            temp--;
    }
    b=a-b-(cnt-1);
    for(int i=0;i<a;i++){
        if(s[i]==' '){
            cout<<" ";
            if(!flag){
                flag=true;
                for(int o=0;o<b/(cnt-1)+b%(cnt-1);o++)
                    cout<<" ";
            }else{
                for(int o=0;o<b/(cnt-1);o++)
                    cout<<" ";
            }
        }else
            cout<<s[i];
    }
    cout<<endl;
}
int main() {
    int a,cnt=0,cnt2=0;
    string s,temp;
    cin>>a;
    getline(cin,s);
    vector<pair<int,string>> v;
    for(int i=0;i<s.size();i++){
        if(s[i]==' '){
            v.push_back(make_pair(cnt,temp));
            temp.clear();
            cnt=0;
        }else{
            temp.push_back(s[i]);
            cnt++;
        }
    }
    for(int i=0;i<v.size();i++){
        
    }
    return 0;
}
